{
  "name": "issue_marionette",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "oxlint src && eslint src",
    "lint:ox": "oxlint src",
    "lint:eslint": "eslint src",
    "lint:fix": "oxlint src --fix && eslint src --fix",
    "format": "oxfmt src --write && prettier --write 'src/**/*.css'",
    "format:check": "oxfmt src && prettier --check 'src/**/*.css'",
    "check": "pnpm run format:check && pnpm run lint && pnpm run test:run",
    "typecheck": "tsc --noEmit",
    "typecheck:file": "tsc-files --noEmit"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@lingui/core": "^5.3.0",
    "@lingui/react": "^5.3.0",
    "@monaco-editor/react": "^4.7.0",
    "@tanstack/react-router": "^1.120.3",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-store": "^2.4.1",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "clsx": "^2.1.1",
    "jotai": "^2.12.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1"
  },
  "devDependencies": {
    "@lingui/cli": "^5.3.0",
    "@lingui/vite-plugin": "^5.3.0",
    "@tanstack/router-devtools": "^1.120.3",
    "@tanstack/router-plugin": "^1.120.3",
    "@tauri-apps/cli": "^2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.2",
    "eslint-config-love": "^140.0.0",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-oxlint": "^1.37.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.0.11",
    "oxfmt": "^0.22.0",
    "oxlint": "^1.37.0",
    "postcss": "^8.5.3",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.17",
    "tsc-files": "^1.1.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.0.4",
    "vitest": "^4.0.16"
  }
}
